                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <!-- Contact pairs to override friction for specific geom combinations -->
  <contact>
    <!-- Low friction between Duplo and floor (so it falls/slides naturally) -->
    <pair geom1="floor" geom2="duplo_body" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_1" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_2" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_3" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_4" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_5" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_6" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_7" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <pair geom1="floor" geom2="stud_8" friction="0.5 0.5 0.005 0.0001 0.0001"/>
    <!-- Low friction between finger pads and floor (so gripper doesn't stick) -->
    <pair geom1="floor" geom2="fixed_pad" friction="0.3 0.3 0.001 0.0001 0.0001"/>
