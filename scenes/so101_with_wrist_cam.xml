<?xml version="1.0" ?>
<!--
SO101 robot model with wrist camera.
Based on so101_new_calib.xml from SO-ARM100 submodule, with wrist camera added.
Mesh files are referenced from the submodule.
-->
<mujoco model="so101_with_wrist_cam">
  <!-- Point to mesh files in submodule -->
  <compiler angle="radian" meshdir="../assets/SO-ARM100/Simulation/SO101/assets" autolimits="true"/>

  <!-- Global simulation options for reliable collision -->
  <option timestep="0.002" impratio="1" gravity="0 0 -9.81" cone="elliptic" noslip_iterations="10"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0" />
    <rgba haze="0.15 0.25 0.35 1" />
    <global azimuth="160" elevation="-20" offwidth="1920" offheight="1080"/>
    <quality shadowsize="4096"/>
  </visual>

  <default>
    <default class="so101_new_calib">
      <joint damping="1" frictionloss="0.1" armature="0.005"/>
      <position kp="50"/>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>
      <default class="collision">
        <geom group="3" contype="3" conaffinity="3"/>
      </default>
    </default>
    <default class="sts3215">
      <geom contype="0" conaffinity="0"/>
      <joint damping="0.60" frictionloss="0.052" armature="0.028"/>
      <position kp="17.8"/>
    </default>
    <default class="backlash">
      <joint damping="0.01" frictionloss="0" armature="0.01" limited="true" range="-0.008726646259971648 0.008726646259971648"/>
    </default>
  <default class="fingerpad">
    <geom
      type="box"
      size="0.004 0.015 0.010"
      friction="100 10 0.1"
      condim="6"
      solref="0.002 1"
      solimp="0.999 0.9999 0.00001"
      contype="3"
      conaffinity="3"
      rgba="0.15 0.15 0.15 0.2"
      group="0"
      priority="1"/>
  </default>
  </default>

  <asset>
    <!-- Textures and materials for scene -->
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072" />
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300" />
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2" />
    <material name="duplo_white" rgba="0.95 0.95 0.95 1" />

    <!-- Robot meshes from submodule -->
    <mesh file="waveshare_mounting_plate_so101_v2.stl"/>
    <mesh file="sts3215_03a_v1.stl"/>
    <mesh file="motor_holder_so101_base_v1.stl"/>
    <mesh file="wrist_roll_follower_so101_v1.stl"/>
    <mesh file="moving_jaw_so101_v1.stl"/>
    <mesh file="base_motor_holder_so101_v1.stl"/>
    <mesh file="upper_arm_so101_v1.stl"/>
    <mesh file="wrist_roll_pitch_so101_v2.stl"/>
    <mesh file="under_arm_so101_v1.stl"/>
    <mesh file="rotation_pitch_so101_v1.stl"/>
    <mesh file="motor_holder_so101_wrist_v1.stl"/>
    <mesh file="sts3215_03a_no_horn_v1.stl"/>
    <mesh file="base_so101_v2.stl"/>

    <!-- Robot materials -->
    <material name="base_motor_holder_so101_v1_material" rgba="1 0.82 0.12 1"/>
    <material name="base_so101_v2_material" rgba="1 0.82 0.12 1"/>
    <material name="sts3215_03a_v1_material" rgba="0.1 0.1 0.1 1"/>
    <material name="waveshare_mounting_plate_so101_v2_material" rgba="1 0.82 0.12 1"/>
    <material name="motor_holder_so101_base_v1_material" rgba="1 0.82 0.12 1"/>
    <material name="rotation_pitch_so101_v1_material" rgba="1 0.82 0.12 1"/>
    <material name="upper_arm_so101_v1_material" rgba="1 0.82 0.12 1"/>
    <material name="under_arm_so101_v1_material" rgba="1 0.82 0.12 1"/>
    <material name="motor_holder_so101_wrist_v1_material" rgba="1 0.82 0.12 1"/>
    <material name="sts3215_03a_no_horn_v1_material" rgba="0.1 0.1 0.1 1.0"/>
    <material name="wrist_roll_pitch_so101_v2_material" rgba="1 0.82 0.12 1"/>
    <material name="wrist_roll_follower_so101_v1_material" rgba="1 0.82 0.12 1"/>
    <material name="moving_jaw_so101_v1_material" rgba="1 0.82 0.12 1"/>
  </asset>

  <worldbody>
    <!-- Scene elements -->
    <light pos="0 0 3.5" dir="0 0 -1" directional="true" />
    <geom name="floor" size="0 0 0.05" pos="0 0 0" type="plane" material="groundplane" contype="1" conaffinity="3"/>

    <!-- Overhead camera (Nuroum V11: 90° diagonal FOV -> ~52° vertical for 16:9) -->
    <camera name="overhead_cam" pos="0.3 -0.09 0.6" axisangle="0 0 1 -1.5708" fovy="52"/>

    <!-- Bowl/container for pick and place target -->
    <!-- Position: moved 10cm right (-Y) -->
    <!-- Square container with thin walls, 12cm x 12cm x 3cm deep -->
    <body name="bowl" pos="0.217 -0.225 0.0">
      <!-- Base (thin plate) -->
      <geom name="bowl_base" type="box" size="0.06 0.06 0.002" pos="0 0 0.002" rgba="0.85 0.85 0.8 1"
        contype="1" conaffinity="3" friction="0.5 0.5 0.01"/>
      <!-- Walls (4 sides) -->
      <geom name="bowl_wall_px" type="box" size="0.003 0.06 0.015" pos="0.06 0 0.017" rgba="0.85 0.85 0.8 1"
        contype="1" conaffinity="3" friction="0.3 0.3 0.01"/>
      <geom name="bowl_wall_nx" type="box" size="0.003 0.06 0.015" pos="-0.06 0 0.017" rgba="0.85 0.85 0.8 1"
        contype="1" conaffinity="3" friction="0.3 0.3 0.01"/>
      <geom name="bowl_wall_py" type="box" size="0.06 0.003 0.015" pos="0 0.06 0.017" rgba="0.85 0.85 0.8 1"
        contype="1" conaffinity="3" friction="0.3 0.3 0.01"/>
      <geom name="bowl_wall_ny" type="box" size="0.06 0.003 0.015" pos="0 -0.06 0.017" rgba="0.85 0.85 0.8 1"
        contype="1" conaffinity="3" friction="0.3 0.3 0.01" mass="0.1"/>
    </body>

    <!-- White Duplo 2x4 brick for pick and place task -->
    <!-- Duplo 2x4: 64mm x 32mm x 19.2mm body + 4.5mm studs -->
    <!-- Position: moved 10cm left (+Y) -->
    <body name="duplo" pos="0.217 0.225 0.0096">
      <freejoint name="duplo_joint"/>
      <!-- Main brick body - high friction for gripping -->
      <geom name="duplo_body" type="box" size="0.032 0.016 0.0096" material="duplo_white" mass="0.03"
        contype="3" conaffinity="3" friction="100 10 0.1" condim="6" margin="0.001"
        solref="0.002 1" solimp="0.999 0.9999 0.00001"/>
      <!-- Studs (8 total: 4x2 grid, 16mm spacing, 9.6mm diameter, 4.5mm tall) -->
      <!-- Row 1 (y = -0.008) -->
      <geom name="stud_1" type="cylinder" size="0.0048 0.00225" pos="-0.024 -0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
      <geom name="stud_2" type="cylinder" size="0.0048 0.00225" pos="-0.008 -0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
      <geom name="stud_3" type="cylinder" size="0.0048 0.00225" pos="0.008 -0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
      <geom name="stud_4" type="cylinder" size="0.0048 0.00225" pos="0.024 -0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
      <!-- Row 2 (y = +0.008) -->
      <geom name="stud_5" type="cylinder" size="0.0048 0.00225" pos="-0.024 0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
      <geom name="stud_6" type="cylinder" size="0.0048 0.00225" pos="-0.008 0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
      <geom name="stud_7" type="cylinder" size="0.0048 0.00225" pos="0.008 0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
      <geom name="stud_8" type="cylinder" size="0.0048 0.00225" pos="0.024 0.008 0.012" material="duplo_white" contype="3" conaffinity="3" friction="100 10 0.1"/>
    </body>

    <!-- Robot arm -->
    <body name="base" pos="0 0 0" quat="1 0 0 0" childclass="so101_new_calib">
      <inertial pos="0.0137179 -5.19711e-05 0.0334843" mass="0.147" fullinertia="0.000114686 0.000136117 0.000130364 -4.59787e-07 4.97151e-06 9.75275e-08"/>
